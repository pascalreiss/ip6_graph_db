MATCH (n)
DETACH DELETE n;

CREATE 
(S1_1:EinfachWeiche {id:'S1_1'}),
(S1:EinfachWeiche {id:'S1'}),
(S2_1:EinfachWeiche {id:'S2_1'}),
(S2:EinfachWeiche {id:'S2'}),

(S1_1)-[:DOUBLE_VERTEX]->(S1),
(S2_1)-[:DOUBLE_VERTEX]->(S2),

(S1)-[:LEADS_TO {distance:20}]->(S2_1),
(S1)-[:LEADS_TO {distance:15}]->(S2_1),
(S2)-[:LEADS_TO {distance:40}]->(S1_1)
